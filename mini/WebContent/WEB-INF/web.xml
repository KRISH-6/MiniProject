<?xml version="1.0" encoding="UTF-8"?>

<web-app>

<display-name>Servlet</display-name>
<display-name>Register</display-name>
<display-name>Login</display-name>

<servlet> 
<servlet-name>Register</servlet-name> 
<servlet-class>niitian.Mainprog</servlet-class> 
</servlet>

<servlet>
<servlet-name>Login</servlet-name> 
<servlet-class>niitian.Mainlog</servlet-class> 
</servlet>




<servlet-mapping> 
<servlet-name>Register</servlet-name> 
<url-pattern>/login/*</url-pattern>  
</servlet-mapping> 

<servlet-mapping> 
<servlet-name>Login</servlet-name>
<url-pattern>/page2/*</url-pattern>
<url-pattern>/page3a/*</url-pattern>
<url-pattern>/page3b/*</url-pattern> 
</servlet-mapping> 

<welcome-file-list> 
<welcome-file>reg.jsp</welcome-file> 
<welcome-file>login.html</welcome-file> 
</welcome-file-list> 

</web-app>
